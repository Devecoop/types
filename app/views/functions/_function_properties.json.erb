
<% connections.each_with_index do |connection, index| %>
    { 
      "uri": "<%=connection.uri%>", 
      "value": "<%=connection.value%>",
      "filter": "<%=connection.filter%>",
      "secret": <%=raw connection.secret.to_json%>
    }<% if (connections.length - 1 > index) %>,<%end%>
<% end %>
